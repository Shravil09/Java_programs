package Practice;

public class Palindrom {

	public static void main(String[] args) {
		int t=121, rev= 0, rem;
		while(t !=0) {
			rem = t%10;
			rev = rev*10 + rem;
			t = t/10;
		}
			if(t==rev) {
			System.out.println(t +" is a palindrome no");
		}
			else
				System.out.println(t +" is not a palindrome no");
	}
}