
public class Int {
	public boolean SameFirstLast(int [] arr) {
		if(arr.length>0 && arr[0]==arr[arr.length-1])
			return true;
		
		return false;
	}

	public static void main(String[] args) {
		int input[] = {1,2,3,1};
		boolean result = new Int().SameFirstLast(input);
		System.out.println(result);
	}

}
