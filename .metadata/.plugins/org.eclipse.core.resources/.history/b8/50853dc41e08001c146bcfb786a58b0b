
public class Double1 {
	public String doubleChar(String str) {
		String S =  "";
		for(int i=0; i<=str.length()-1;i++) 
		{
			S +=str.charAt(i);
			S +=str.charAt(i);
		}
		return S;
		}
	public static void main(String[] args) {
	String result = new Double1().doubleChar("Shravil");
	System.out.println(result);
		}
	
}